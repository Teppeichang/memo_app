<div class="main users-new">
  <div class="container">
    <h2 class="form-heading">新規ユーザー登録</h2>
    <%= form_tag("/users/create")do %>
      <div class="form users-form">
        <p>ユーザー名</p>
        <input name="name">
        <p>メールアドレス</p>
        <input name="email">
        <p>パスワード</p>
        <input name="password" type="password" value="<%= @user.password %>">
        <input type="submit" value="ユーザー登録">
      </div>
    <% end %>
  </div>
</div>